{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react",
    "paths": {
      "@/*": ["src/*"],
      "utils/*": ["src/utils/*"],
      "pages/*": ["src/pages/*"],
      "components/*": ["src/components/*"],
      "common/*": ["src/common/*"],
      "services/*": ["src/services/*"],
      "store/*": ["src/store/*"]
    },
    "target": "ES6",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true
  },
  "strict": false
}
