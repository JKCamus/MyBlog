{
  "compilerOptions": {
    "jsx": "react-jsx",
    "paths": {
      "@/*": ["src/*"],
      "utils/*": ["src/utils/*"],
      "pages/*": ["src/pages/*"],
      "components/*": ["src/components/*"],
      "services/*": ["src/services/*"],
      "store/*": ["src/store/*"]
    },
    "target": "ES6",
    "module": "ESNext",
    "allowSyntheticDefaultImports": true,
    "typeRoots": ["node_modules/@types"],
    "strict": false
  }
}
