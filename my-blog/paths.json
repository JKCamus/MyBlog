{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "utils/*": ["src/utils/*"],
      "pages/*": ["src/pages/*"],
      "components/*": ["src/components/*"],
      "services/*": ["src/services/*"],
      "store/*": ["src/store/*"],
    },
    "target": "ES6",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true
  }
}
